{
  "name": "api-arbitraje",
  "version": "1.1.0",
  "description": "API para arbitraje",
  "engines": {
    "node": ">=10.7.x",
    "npm": ">=6.2.0",
    "pm2": ">=3.0.3"
  },
  "main": "index.js",
  "scripts": {
    "check-node": "check-node-version --package",
    "start": "npm run dev",
    "dev": "npm run build",
    "clean": "rm -rf dist",
    "build": "webpack && pm2 start process.json",
    "build-w": "webpack && pm2 start process.json",
    "build:qa": "npm run clean && npm run check-node && webpack",
    "build:pro": "npm run clean && npm run check-node && webpack -p",
    "production": "npm run build:pro && node bin/production",
    "test": "nodemon --exec babel-node server/index.js"
  },
  "keywords": [
    "node",
    "api"
  ],
  "author": "FMF",
  "license": "ISC",
  "private": true,
  "repository": "",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "check-node-version": "^3.2.0",
    "nodemon": "^1.19.1",
    "webpack": "^4.47.0",
    "webpack-cli": "^3.3.12",
    "webpack-node-externals": "^1.6.0",
    "webpack-shell-plugin": "^0.5.0"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "body-parser": "^1.18.3",
    "chalk": "^2.4.1",
    "connect-busboy": "1.0.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "format-duration": "^2.0.0",
    "glob": "^7.1.2",
    "group-array": "^1.0.0",
    "handlebars": "^4.7.7",
    "helmet": "^3.13.0",
    "lodash": "^4.17.21",
    "log4js": "^4.3.2",
    "moment": "^2.22.2",
    "moment-timezone": "^0.5.21",
    "mongoose": "^6.13.0",
    "morgan": "^1.9.0",
    "mysql": "^2.18.1",
    "path": "^0.12.7",
    "winston": "^2.4.0"
  }
}
